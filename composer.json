{
    "name": "codespb/livewire-notifier",
    "description": "Simple Livewire notifications system without any dependencies except Tailwind CSS",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitriy Belyaev",
            "email": "cuamckuy@gmail.com",
            "homepage": "https://cogear.ru"
        }
    ],
    "homepage": "https://github.com/codespb/livewire-notifier",
    "keywords": ["Laravel", "LivewireNotifier","Toast","Growl","Livewire"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CodeSPB\\LivewireNotifier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeSPB\\LivewireNotifier\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeSPB\\LivewireNotifier\\LivewireNotifierServiceProvider"
            ],
            "aliases": {
                "LivewireNotifier": "CodeSPB\\LivewireNotifier\\Facades\\LaravelLivewireNotifier"
            }
        }
    }
}
